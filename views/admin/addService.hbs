<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Add</title>
  </head>
  <body>
    <nav class="navbar navbar-dark bg-light">
      <h4>VenueBook</h4>

    </nav>
    <div class="container mt-5">
      <div class="row">
        <div class="col">
          <form>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="inputEmail4">Service</label>
                <input
                  type="text"
                  name="service"
                  class="form-control"
                  id="inputEmail4"
                  placeholder="E.g DJ,"
                />
              </div>
              <div class="form-group col-md-6">
                <label for="inputPassword4">Contact</label>
                <input
                  type="text"
                  name="contact"
                  class="form-control"
                  id="inputPassword4"
                  placeholder="0712345678"
                />
              </div>
            </div>
            <div class="form-group">
              <label for="inputAddress">Cost</label>
              <input
                type="text"
                name="cost"
                class="form-control"
                id="inputAddress"
                placeholder="In Ksh."
              />
            </div>
            <button type="submit" class="btn btn-success">ADD</button>
            <button type="submit" class="btn btn-danger">CANCEL</button>
          </form>
          <a href="/admin/services" type="button">Back</a>
        </div>
      </div>
    </div>

  </body>
</html>